   "source": [
    "def remove_smallest(numbers):\n",
    "    if not numbers:\n",
    "        return [ ]\n",
    "    smallest = min(numbers)\n",
    "    smallest_index = numbers.index(smallest)\n",
    "    return numbers[:smallest_index] + numbers[smallest_index+1:]\n",
    "\n",
    "remove_smallest([361, 223, 311, 268, 3, 269, 185, 163, 9])"
   ]
  }
