    "def wave(people):\n",
    "    wave = []\n",
    "    for i in range(len(people)):\n",
    "        if people[i].isalpha():\n",
    "            new_word = people[:i].lower() + people[i].upper() + people[i+1:].lower()\n",
    "            wave.append(new_word)\n",
    "    return wave\n",
    "\n",
    "\n",
    "            \n",
    "wave('two words')"
